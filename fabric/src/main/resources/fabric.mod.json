{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    "Wesley1808",
    "FallenBreath",
    "NoComment",
    "LittleLily",
    "ThatTrollzer",
    "Kichura",
    "ellieisjelly"
  ],
  "contact": {
    "sources": "https://github.com/DragonEggBedrockBreaking/VanillaDisable",
    "issues": "https://github.com/DragonEggBedrockBreaking/VanillaDisable/issues"
  },
  "license": "${license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "uk.debb.vanilla_disable.LangFileManager"
    ]
  },
  "mixins": [
    "vanilla_disable.mixins.json",
    "vanilla_disable.fabric.mixins.json"
  ],
  "accessWidener": "vanilla_disable.aw",
  "depends": {
    "minecraft": "${minecraft_version_range_fabric}",
    "fabricloader": "${fabric_loader_version_range}"
  },
  "suggests": {
    "fabric-resource-loader-v0": "*"
  }
}
